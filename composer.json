{
    "name": "o2o/fluent-fm",
    "description": "A PHP package for FileMaker Server's Data API using a fluent query builder style interface.",
    "type": "library",
    "authors": [
        {
            "name": "Antenna",
            "email": "info@weareantenna.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "o2o\\FluentFM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests",
          "Illuminate\\Support\\Facades\\": "tests/Stubs"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "ramsey/uuid": "^4.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13",
        "phpunit/phpunit": "^9.6",
        "doctrine/coding-standard": "^13.0",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
